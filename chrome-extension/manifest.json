{
  "manifest_version": 2,

  "name": "Prolix",
  "description": "This extension demonstrates a browser action with kittens.",
  "version": "1.0",
  "background": {"scripts": ["background.js"]},
  "permissions": [
    "https://secure.flickr.com/",
    "tabs"
  ],
  "page_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
